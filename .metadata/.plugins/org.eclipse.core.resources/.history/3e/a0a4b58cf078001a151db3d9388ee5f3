package Polymorphisim;



class A{
	private String name;
	public A(String name) {
		
		this.name = name;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void konus() {
		
		System.out.println(this.name + " calisiyor");
	}
	
}

class B extends A{
	private String name;

	public B(String name) {
		
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public void konus() {
		// TODO Auto-generated method stub
		System.out.println(this.name + "working");
	}
	
	
	
}

class C extends A{
	private String name;

	public C(String name) {
		
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public void konus() {
		// TODO Auto-generated method stub
		System.out.println(this.name + "yatiyor");
	}
	
	
	
}
public class Main {
	public static void main(String args[]) {
	A a = new A("ferdi");
	A a = new B("ferdi");
	A a = new C("ferdi");
	a.konus();
	
	}	
}
