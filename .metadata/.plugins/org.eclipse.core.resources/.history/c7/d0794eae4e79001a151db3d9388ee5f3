package Cards;

import java.util.Scanner;

public class Main {
public static void main(String args[]) {
	int[][] kartlar = new int[4][4];
	Scanner scan = new Scanner(System.in);
	
	for(int i=0; i<4; i++) {
		for(int j=0; j<4; j++) {
			System.out.print(i +"," +  j + ":" );
			kartlar[i][j] = scan.nextInt[][];
		}
		
	}
}
}
